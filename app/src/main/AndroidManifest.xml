<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.homefitp">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AgregarEjercicioActivity"></activity>
        <activity android:name=".RealizarRutinaActivity" />
        <activity android:name=".DetalleEjercicioActivity" />
        <activity android:name=".DetalleRutinaActivity" />
        <activity android:name=".ConfiguracionActivity" />
        <activity android:name=".EditarDatosActivity" />
        <activity android:name=".RegistroDatosActivity" />
        <activity android:name=".PrincipalActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
    </application>

</manifest>